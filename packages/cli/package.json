{
  "name": "sap-devspace-cli",
  "description": "An editor-agnostic, pure CLI adaptation of the SAP Business Application Studio Toolkit extension for VSCode.",
  "version": "0.0.5",
  "license": "Apache-2.0",
  "module": "./src/index.tsx",
  "type": "module",
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "@types/deno": "^2.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/semver": "^7.7.0",
    "@types/which": "^3.0.4",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.25.8",
    "node-rsa": "^1.1.1"
  },
  "engines": {
    "node": ">=23.8.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.1",
    "@inkjs/ui": "^2.0.0",
    "@microsoft/dev-tunnels-ssh": "^3.12.5",
    "@microsoft/dev-tunnels-ssh-tcp": "^3.12.5",
    "@noble/curves": "^1.9.6",
    "@sap/bas-sdk": "^3.11.6",
    "@std/ulid": "npm:@jsr/std__ulid@^1.0.0-rc.4",
    "chalk": "^5.5.0",
    "fast-npm-meta": "^0.4.6",
    "figures": "^6.1.0",
    "fs-extra": "^11.3.1",
    "hono": "^4.9.0",
    "ink": "^6.1.0",
    "ink-spinner": "^5.0.0",
    "meow": "^13.2.0",
    "open": "^10.2.0",
    "package-manager-detector": "^1.3.0",
    "react": "^19.1.1",
    "semver": "^7.7.2",
    "ssh-config": "^5.0.3",
    "which": "^5.0.0",
    "ws": "^8.18.3"
  },
  "scripts": {
    "build": "npx tsx ../../scripts/build.mts",
    "build:bun": "bun run ../../scripts/build.mts",
    "dev": "npx tsx src/index.tsx",
    "dev:spy": "npx tsx src/spy.ts",
    "dev:bun:spy": "bun src/spy.ts",
    "dev:deno:spy": "deno run -A src/spy.ts",
    "dev:watch": "npx tsx --watch src/index.tsx",
    "dev:bun:watch": "bun --watch src/index.tsx",
    "dev:deno:watch": "deno run -A --watch src/index.tsx",
    "dev:test": "NODE_ENV=development npx tsx src/index.tsx",
    "dev:deno:test": "NODE_ENV=development deno run -A src/index.tsx",
    "test": "npm run build && dist/bin/index.js",
    "test:bun": "bun run build:bun && dist/bin/index.js",
    "test:errors": "NODE_ENV=development npx tsx src/test/testSuite.tsx",
    "test:error-boundaries": "NODE_ENV=development npx tsx src/test/errorBoundaryTests.tsx",
    "test:error-scenarios": "NODE_ENV=development npx tsx src/test/runErrorScenarios.tsx",
    "test:posthog": "NODE_ENV=development npx tsx src/test/runPostHogTest.tsx",
    "test:quick": "NODE_ENV=development npx tsx src/test/quickTest.tsx",
    "test:bun:errors": "NODE_ENV=development bun src/test/testSuite.tsx",
    "test:bun:error-boundaries": "NODE_ENV=development bun src/test/errorBoundaryTests.tsx",
    "test:bun:error-scenarios": "NODE_ENV=development bun src/test/runErrorScenarios.tsx",
    "test:bun:posthog": "NODE_ENV=development bun src/test/runPostHogTest.tsx",
    "test:bun:quick": "NODE_ENV=development bun src/test/quickTest.tsx",
    "test:deno:errors": "NODE_ENV=development deno run -A src/test/testSuite.tsx",
    "test:deno:error-boundaries": "NODE_ENV=development deno run -A src/test/errorBoundaryTests.tsx",
    "test:deno:error-scenarios": "NODE_ENV=development deno run -A src/test/runErrorScenarios.tsx",
    "test:deno:posthog": "NODE_ENV=development deno run -A src/test/runPostHogTest.tsx",
    "test:deno:quick": "NODE_ENV=development deno run -A src/test/quickTest.tsx"
  },
  "optionalDependencies": {
    "posthog-node": "^5.6.0"
  },
  "files": [
    "bin",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielNappa/sap-devspace-cli.git"
  },
  "bugs": {
    "url": "https://github.com/DanielNappa/sap-devspace-cli/issues"
  },
  "homepage": "https://github.com/DanielNappa/sap-devspace-cli#readme",
  "keywords": [
    "sap",
    "bas",
    "business application studio",
    "dev space",
    "ssh",
    "sap ssh tunnel",
    "cli",
    "btp"
  ]
}
